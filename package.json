{
  "name": "visual-testing-demo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "visual-test": "npm-run-all visual-test:only-capture-and-assert; npm run visual-test:only-report",
    "visual-test:only-capture-and-assert": "loki test --chromeFlags=\"--headless --no-sandbox --disable-gpu --hide-scrollbars\"",
    "visual-test:approve": "loki approve",
    "visual-test:only-report": "npm-run-all visual-test:only-report:*",
    "visual-test:only-report:json": "node .loki/generate-visual-json-report.js",
    "visual-test:only-report:html": "reg-cli --from .loki/report.json --report .loki/report.html"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.2.0",
    "@angular/cdk": "^13.2.5",
    "@angular/common": "~13.2.0",
    "@angular/compiler": "~13.2.0",
    "@angular/core": "~13.2.0",
    "@angular/forms": "~13.2.0",
    "@angular/material": "^13.2.5",
    "@angular/platform-browser": "~13.2.0",
    "@angular/platform-browser-dynamic": "~13.2.0",
    "@angular/router": "~13.2.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.2.3",
    "@angular/cli": "~13.2.3",
    "@angular/compiler-cli": "~13.2.0",
    "@babel/core": "^7.17.5",
    "@compodoc/compodoc": "^1.1.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addons": "^6.4.19",
    "@storybook/angular": "^6.4.19",
    "@storybook/builder-webpack5": "^6.4.19",
    "@storybook/manager-webpack5": "^6.4.19",
    "@storybook/testing-library": "^0.0.9",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "babel-loader": "^8.2.3",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "loki": "^0.30.2",
    "npm-run-all": "^4.1.5",
    "reg-cli": "^0.17.5",
    "storybook-addon-designs": "^6.2.1",
    "typescript": "~4.5.2"
  }
}
